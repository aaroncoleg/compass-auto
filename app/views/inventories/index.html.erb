<p id="notice"><%= notice %></p>

<h1>Inventories</h1>

<table>
  <thead>
    <tr>
      <th>Miles</th>
      <th>Year</th>
      <th>Model</th>
      <th>Vin</th>
      <th>Sold</th>
      <th>Price</th>
      <th>Color</th>
      <th>Make</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inventories.each do |inventory| %>
      <tr>
        <td><%= inventory.miles %></td>
        <td><%= inventory.year %></td>
        <td><%= inventory.model %></td>
        <td><%= inventory.vin %></td>
        <td><%= inventory.sold %></td>
        <td><%= inventory.price %></td>
        <td><%= inventory.color %></td>
        <td><%= inventory.make %></td>
        <td><%= link_to 'Show', inventory %></td>
        <td><%= link_to 'Edit', edit_inventory_path(inventory) %></td>
        <td><%= link_to 'Destroy', inventory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inventory', new_inventory_path %>
